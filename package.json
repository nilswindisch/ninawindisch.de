  "bugs": {
    "url": "https://github.com/nilswindisch/ninawindisch.de/issues"
  },
  "homepage": "https://github.com/nilswindisch/ninawindisch.de#readme",
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "concurrently": "^3.4.0",
    "cssnano": "^3.10.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss": "^7.0.10",
    "postcss-cli": "^5.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0"
  }
}
