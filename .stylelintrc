{
  "extends": "stylelint-config-standard",
  "plugins": ["stylelint-order"],
  "rules": {
      "order/order": [
        {
            type: "at-rule",
            name: "include"
        },
        "declarations",
        "rules",
        {
            type: "at-rule",
            name: "media"
        }
      ],
      "order/properties-alphabetical-order": true,
    "declaration-empty-line-before": null,
    "max-empty-lines": 3
  }
 }
